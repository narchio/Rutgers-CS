<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<Pane fx:controller="Controller.LoginController" prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="275.0" layoutY="150.0" prefHeight="60.0" prefWidth="450.0" text="Photo Application Login">
         <font>
            <Font size="45.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="387.5" layoutY="325.0" prefHeight="40.0" prefWidth="225.0" promptText="Enter username...">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button onAction="#login" fx:id="loginButton" layoutX="450.0" layoutY="400.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Login">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</Pane>
